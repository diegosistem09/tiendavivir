/**
 * @file
 * The default format for Colombian adresses.
 */

$plugin = array(
  'title' => t('Address form (specific for Colombia)'),
  'format callback' => 'addressfield_co_format_address_generate',
  'type' => 'address',
  'weight' => -90,
);

/**
 * Format callback for Colombia address.
 *
 * @see CALLBACK_addressfield_format_callback()
 */
function addressfield_co_format_address_generate(&$format, $address, $context = array()) {
  if ($address['country'] == 'CO') {
    // Remove second address.
    unset($format['street_block']['premise']);

    // Colombian states with DANE codes.
    $format['locality_block']['administrative_area'] = array(
      '#title' => t('State'),
      '#empty_option' => t('- Select state -'),
      '#required' => TRUE,
      '#options' => addressfield_co_states_list(),
      '#render_option_value' => TRUE,
    );
    $format['locality_block']['locality'] = array(
      '#title' => t('City'),
      '#empty_option' => t('- Select city -'),
      '#options' => array(),
      '#required' => TRUE,
      '#disabled' => TRUE,
      '#attributes' => array('class' => array('locality')),
      '#weight' => 1,
      '#render_option_value' => TRUE,
    );

    // If a state has been selected then enable locality and load cities.
    if (!empty($address['administrative_area'])) {
      $format['locality_block']['locality']['#disabled'] = FALSE;
      $format['locality_block']['locality']['#options'] = addressfield_co_cities_list($address['administrative_area']);
    }

    // If render address add a suffix.
    if ($context['mode'] == 'render') {
      $format['locality_block']['administrative_area']['#suffix'] = ', ';
    }

    // Check ajax form.
    if ($context['mode'] == 'form') {
      $format['locality_block']['administrative_area']['#ajax'] = array(
        'callback' => 'addressfield_standard_widget_refresh',
        'wrapper' => $format['#wrapper_id'],
        'method' => 'replace',
      );
    }
    else {
      // Cancel the AJAX for forms we don't control.
      if (isset($format['locality_block']['administrative_area'])) {
        $format['locality_block']['administrative_area']['#ajax'] = array();
      }
    }
  }
}

